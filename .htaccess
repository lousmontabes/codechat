RewriteEngine On    # Turn on the rewriting engine

RewriteCond %{HTTP_HOST} ^codechat\.co$
RewriteRule ^(.*)$ http://www.example.co/$1 [R=301]

RewriteRule    ^([A-Za-z0-9]+)\/?$    chatbox.php?token=$1    [NC,L]    # Handle requests for chatrooms

RewriteCond %{THE_REQUEST} ^GET.*home\.php [NC]
RewriteRule    ^([A-Za-z0-9]+)\/?$    home.php    [NC,L]    # Handle requests for chatrooms